{
  "name": "eslint-config-blockabc",
  "version": "0.13.0",
  "description": "A All-in-One eslint config project based on Standard Style",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:javascript": "eslint -c index.js test/javascript.js",
    "test:typescript": "eslint -c typescript.js test/typescript.ts",
    "test:vue": "eslint -c vue.js test/vue.vue test/javascript.js",
    "test:nuxt": "eslint -c nuxt.js test/nuxt.vue test/javascript.js test/nuxt.config.js",
    "test:vue-ts": "eslint -c vue-ts.js test/vue-ts.vue test/typescript.ts",
    "test:nuxt-ts": "eslint -c nuxt-ts.js test/nuxt-ts.vue test/typescript.ts"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "standard",
    "blockabc",
    "typescript",
    "vue",
    "nuxt",
    "blockchain"
  ],
  "author": "JeffJing",
  "repository": {
    "type": "git",
    "url": "git@github.com:BlockABC/eslint-config-blockabc.git"
  },
  "homepage": "https://github.com/BlockABC/eslint-config-blockabc",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^7.30.0",
    "typescript": "^4.3.5",
    "@babel/eslint-parser": "^7.14.7"
  },
  "devDependencies": {
    "@types/node": "^16.3.2",
    "@babel/eslint-parser": "^7.14.7",
    "eslint": "^7.30.0",
    "nuxt": "^2.15.7",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.13.0",
    "vue-eslint-parser": "^7.8.0"
  }
}
