{
  "name": "eslint-config-blockabc",
  "version": "0.11.3",
  "description": "A All-in-One eslint config project based on Standard Style",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:javascript": "eslint -c index.js test/javascript.js",
    "test:typescript": "eslint -c typescript.js test/typescript.ts",
    "test:vue": "eslint -c vue.js test/vue.vue test/javascript.js",
    "test:nuxt": "eslint -c nuxt.js test/nuxt.vue test/javascript.js test/nuxt.config.js",
    "test:vue-ts": "eslint -c vue-ts.js test/vue-ts.vue test/typescript.ts",
    "test:nuxt-ts": "eslint -c nuxt-ts.js test/nuxt-ts.vue test/typescript.ts"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "standard",
    "blockabc",
    "typescript",
    "vue",
    "nuxt",
    "blockchain"
  ],
  "author": "JeffJing",
  "repository": {
    "type": "git",
    "url": "git@github.com:BlockABC/eslint-config-blockabc.git"
  },
  "homepage": "https://github.com/BlockABC/eslint-config-blockabc",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^7.3.1",
    "typescript": "^3.9.5",
    "babel-eslint": "^10.1.0"
  },
  "devDependencies": {
    "eslint": "^7.3.1",
    "typescript": "^3.9.5",
    "babel-eslint": "^10.1.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "vue-eslint-parser": "^7.1.0"
  }
}
